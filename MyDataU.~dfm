  object qryCllist: TMyQuery
    Connection = myconn
    SQL.Strings = (
      
        'SELECT ClCode, Section, AdmNo, Name, RollNo, Fname, DOB,Phone, S' +
        'EX'
      'FROM studmast'
      ' where ClCode=:ccode and Section=:sec and Catcode<>0 '
      'ORDER BY ClCode, Section, AdmNo')
    Left = 713
    Top = 208
