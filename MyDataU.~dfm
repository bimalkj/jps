  object qryBank: TMyQuery
    Connection = myconn
    SQL.Strings = (
      'SELECT Fee.CHQ_NO, Bmast.Descr, Brmast.Descr, Fee.CHQ_AMT'
      'FROM Fee'
      '   INNER JOIN  Bmast'
      '   ON  (Bmast.Bcode = Fee.DRAWN_ON)  '
      '   INNER JOIN  Brmast'
      '   ON  (Brmast.Bcode = Fee.Branch)  '
      'WHERE  Fee.CHQ_NO <> '#39#39' '
      'and MR_DATE between :sdate and :edate')
    Left = 609
    Top = 208
    ParamData = <
