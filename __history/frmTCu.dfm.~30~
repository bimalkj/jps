  object Query1: TMyQuery
    Connection = MyData.myconn
    SQL.Strings = (
      'SELECT tcdata.admno,tcno,name'
      'FROM tcdata inner join studmast on tcdata.admno=studmast.studid'
      'WHERE  UPPER(CAST('
      'tcdata.admno'
      'AS CHAR(40))) LIKE :empno')
    Left = 8
    Top = 8
    ParamData = <
      item
        DataType = ftString
        Name = 'empno'
      end>
    object Query1admno: TStringField
      FieldName = 'admno'
      Size = 10
    end
    object Query1tcno: TIntegerField
      FieldName = 'tcno'
    end
    object Query1name: TStringField
      FieldName = 'name'
      Size = 40
    end
  end
