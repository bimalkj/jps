  object Query1: TMyQuery
    Connection = MyData.myconn
    SQL.Strings = (
      'SELECT StudId,Name'
      'FROM studmast'
      'WHERE  UPPER(CAST('
      'StudId'
      'AS CHAR(40))) LIKE :empno')
    Left = 8
    Top = 8
    ParamData = <
      item
        DataType = ftString
        Name = 'empno'
      end>
    object Query1StudId: TFloatField
      FieldName = 'StudId'
    end
    object Query1Name: TStringField
      FieldName = 'Name'
      Size = 40
    end
  end
end
